{
  "presets": [
    ["env", {
      "targets": {
        "browsers": [
          "last 2 versions",
          "ie >= 9"
        ]
      },
      "modules": false
    }],
    "react"
  ],
  "plugins": ["transform-object-rest-spread"],
  "env": {
    "test": {
      "plugins": [
        "transform-es2015-modules-commonjs"
      ]
    }
  }
}
